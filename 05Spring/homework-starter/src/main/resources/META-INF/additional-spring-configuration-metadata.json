      "type": "java.lang.String",
      "description": "student's name.",
      "defaultValue": "Zhang"
    },
    {
      "name": "spring.homework.student.age",
      "type": "java.lang.Integer",
      "description": "student's age.",
      "defaultValue": 18
    },
    {
      "name": "spring.homework.xclass.name",
      "type": "java.lang.String",
